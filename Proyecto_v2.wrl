# MODELADO DE BARRACA 04

## IMPORTAR PROTOTIPOS

EXTERNPROTO	Caja [
	field SFVec3f posicion
	field SFRotation rotacion
	field SFVec3f escala
] "Caja.wrl#Caja"

EXTERNPROTO	Escritorio [
	field SFVec3f posicion
	field SFVec3f escala
] "Escritorio.wrl#Escritorio"

EXTERNPROTO	Pizarra [
	field SFVec3f posicion
	field SFColor colorMarco
	field SFVec3f escala
] "Pizarra.wrl#Pizarra"

EXTERNPROTO	Pupitre [
	field SFVec3f posicion
	field SFVec3f escala
	field SFRotation rotacion
] "Pupitre.wrl#Pupitre"

EXTERNPROTO	AmbienteBarraca [
	field SFVec3f posicion
	field MFString aula
	field SFInt32 openCortina
	field SFInt32 openVentana
] "AmbienteBarraca.wrl#AmbienteBarraca"

EXTERNPROTO	Afiche [
	field SFVec3f posicion
	field SFVec3f escala
	field MFString ruta
] "Afiche.wrl#Afiche"

